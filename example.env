# server's open connection
TG_MAX_CONNECTION=100
# Deta timeout
TG_TIMEOUT=10

# Parameters for webhook mode:
WEBHOOK_DOMAIN=https://test.deta.dev
WEBHOOK_PATH=/bot
WEBHOOK_PORT=443
WEBHOOK_TMPL={WEBHOOK_URL}:{WEBHOOK_PORT}{WEBHOOK_PATH}

